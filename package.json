  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "Electron_electron-dev": "concurrently \"BROWSER=none yarn web\" \"wait-on http://localhost:19006 && electron electron/index.js\"",
    "Electron_postinstall": "electron-builder install-app-deps",
    "Electron_electron-pack": "build -mw",
    "runSimulator": "/Applications/Xcode.app/Contents/Developer/Applications/Simulator.app/Contents/MacOS/Simulator -CurrentDeviceUDID E757B951-D380-4423-B64A-079C54652C9D",
    "build:types": "tsc -p . && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "rm -rf dist && yarn build:js && yarn build:types",
    "clearCache": "watchman watch-del-all",
    "buildFunctions": "rm -rf functions-build && yarn buildFunctions:js",
    "buildFunctions:js": "babel ./firebase/functions -d functions-build --extensions \".js,.jsx,.ts,.tsx\" --copy-files && cd functions-build && npm install && cd ..",
    "clear": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install && yarn start",
    "test": "jest",
    "dev": "nodemon src/main.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build:js": "yarn version --patch && babel src -d dist --config-file ./esBabel.config.js --extensions \".js,.jsx,.ts,.tsx\" --copy-files && cp ./package.json ./dist",
    "fire": "yarn build && cd dist && npm publish",
    "auth": "cd dist && npm login --always-auth && cd ..",
    "storybook": "start-storybook -c ./.storybook -s ./.storybook/public -p 9009",
    "build-storybook": "build-storybook -c ./.storybook -o storybook-build",
    "docs": "start-storybook -c ./.storybookDocs -s ./.storybookDocs/public -p 9010",
    "build-docs": "build-storybook -c ./.storybookDocs -o docs-build",
    "chromatic": "chromatic --project-token 1un5jqj6bcs"
  },
  "dependencies": {
